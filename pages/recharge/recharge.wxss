/* pages/recharge/recharge.wxss */

/* 顶部全局样式 */

page {
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}

/* 顶部标题 */

.top{
  width: 750rpx;
  padding: 30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  font-size: 30rpx;
  color: #333333;
}

/* 充值列表 */

.content_body{
  width: 750rpx;
  box-sizing: border-box;
  font-size: 34rpx;
  color: #333333;
}

.content_list{
  float: left;
}

.content_item{
  width: 200rpx;
  height: 70rpx;
  font-size: 34rpx;
  color: #333333;
  /* box-shadow: 0rpx 0rpx 5rpx 0rpx #E2E2E2; */
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx 25.5rpx 15rpx 25.5rpx;
}

.backgroud_img_select{
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.backgroud_img_noselect{
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

/* 手动输入 */

.enter{
  width: 750rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #333333;
  margin-top: 30rpx;
}

.inputstyle{
  width: 200rpx;
  border-bottom: 2rpx solid #E2E2E2;
}

.inputstyle input{
  padding-left: 17rpx;
}

.input_style{
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

/* 底部确认充值按钮 */

.buttons{
  width: 750rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.buttons button{
  width: 600rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* text-align: center; */
  background-color: #41BA53;
  color: white;
  border-radius: 44rpx;
  margin-top: 60rpx;
}
.detailedTitle{
  background-color: #f5f5f5;
  text-align: center;
  margin: 30rpx 30rpx 20rpx 30rpx;
  letter-spacing: 4rpx;
  font-size: 34rpx;
  color: #666;
  line-height: 66rpx;
}
.detailedList{
  /* background-color: aqua; */
  width: 500rpx;
  margin: 0 auto;
}
.detailed{
  color: #F74603;
  font-size: 30rpx;
}
.listName{
  /* background-color: bisque; */
  font-size: 26rpx;
  color: #666;
  display: inline-block;
  /* text-align: justify; */
  /* text-align-last: justify; */
}
.listmoney{
  /* display: inline-block; */
  float: right;
}