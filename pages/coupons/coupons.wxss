/* pages/coupons/coupons.wxss */

/* 顶部公用组件 */

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}

/* 页面全局样式 */
page {
  background-color: #ededee
}

/* 顶部标签页 */

.current {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #333333;
  font-size: 30rpx;
  margin-bottom: 15rpx;
}

.item {
  width: 100%;
  text-align: center;
}

.buttom_linear {
  color: #41BA53;
  display: inline-block;
}

/* 优惠券框 */

.coupons {
  width: 690rpx;
  height: 206rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  margin: 15rpx auto;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.coupons_top {
  width: 522rpx;
  height: 165rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.coupons_top_left {
  font-size: 60rpx;
  color: #41BA53;
}

.coupons_top_left text {
  font-size: 30rpx;
  color: #41BA53;
}

.coupons_top_right {
  font-size: 20rpx;
  color: #999999;
  margin-left: 40rpx;
}

.coupons_buttom {
  width: 522rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: #999999;
}


/* 指定商品 */

.specify {
  width: 690rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  margin: 0 auto;
}

.specifyItem {
  width: 340rpx;
  height: 570rpx;
  margin: 15rpx 0;
  float: left;
  margin: 16rpx;
}

.specifyItem_top {
  height: 120rpx;
  color: #41BA53;
  font-size: 40rpx;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}

.specifyItem_top image {
  width: 340rpx;
  height: 120rpx;
}

.product {
  width: 280rpx;
  height: 390rpx;
  background-color: white;
  border-radius: 20rpx;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
}

.product_img image {
  width: 281rpx;
  height: 281rpx;
}

.product_tit {
  font-size: 24rpx;
  color: #333333;
  font-weight: bold;
  width: 274rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}



.pp {
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.oo {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.newimg {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.limit {
  background-image: url(data:image/png;base64,)
}


/* 新css */
/* 左侧分类列表 */
.sortList {
  width: 220rpx;
}

.sortList_item {
  width: 220rpx;
  height: 120rpx;
  font-size: 28rpx;
  background-color: #F8F8F8;
  display: flex;
  justify-content: center;
  align-items: center;

}

.sortList_item_s {
  border-left: 8rpx solid #41BA53;
  background-color: white;
}

/* 右侧内容 */
.productList {
  width: 530rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

.contentList {
  display: flex;
  flex-direction: row;
}

.sTab {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  background-color: #F8F8F8;
  width: 256rpx;
  height: 58rpx;
  margin:20rpx;
}

.productList_item:first-child {
  margin-top: 0;
}

.productList_item {
  position: relative;
  width: 94%;
  height: 146rpx;
  margin: 20rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.couponInfor {
  /* top: 15rpx;
  left: 20rpx; */
}

.money,
.infor,
.time,
.moneySysbol,
.couponName {
  display: inline-block;
  font-size: 15rpx;
  color: #999999;
}

.infor {
  margin-left: 6rpx;
  position:absolute;
  bottom:56rpx;
  right:140rpx;
}

.couponName{
  position: absolute;
  top: 20rpx;
  right: 140rpx;
  color: #666;
  font-size: 24rpx;
}

.moneySysbol {
  color: #41BA53;
}

.money {
  font-size: 62rpx;
  color: #41BA53;
  position:absolute;
  top:18rpx;
  left:22rpx;
}

.time {
  position: absolute;
  bottom: 7rpx;
  left: 16rpx;
}

/* 指定商品 */
.appoint_item {
  display: inline-block;
  margin: 0 16rpx 16rpx 16rpx;
  width: 230rpx;
}

.appoint_top {
  width: 100%;
  height: 122rpx;
  /* background-color: red; */
  position: relative;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.appoint_bottom {
  height: 300rpx;
  background-color:white;
  padding-top: 14rpx;
}

.appoint_money {
  font-size: 34rpx;
  color: #41BA53;
  position: absolute;
  left: 12rpx;
  top: 10rpx;
}

.appoint_btn {
  color: #41BA53;
  border-radius: 10rpx;
  border: 1rpx solid #41ba41;
  position: absolute;
  width: 100rpx;
  height: 34rpx;
  right: 16rpx;
  text-align: center;
  line-height: 34rpx;
  top: 16rpx;
}

.appoint_btn,
.appoint_timeName,
.appoint_time,
.appointName {
  font-size: 20rpx;
}

.appoint_timeName {
  position: absolute;
  left: 20rpx;
  top: 64rpx;
  color:#999999;
}

.appoint_time {
  position: absolute;
  bottom: 6rpx;
  left: 60rpx;
  color:#999999;
}

.appoint_imgBox {
  background-color: gold;
  width: 200rpx;
  height: 200rpx;
  margin: 0 14rpx 14rpx 14rpx;
}
.appoint_imgBox image{
  width:100%;
  height:100%;
}

.appointName {
  padding: 0 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}