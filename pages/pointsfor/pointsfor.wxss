/* pages/pointsfor/pointsfor.wxss */

/* 全局页面样式 */

page {
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #ededee;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}


/* 顶部标签页 */

.current{
  width: 100%;
  background-color: white;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #333333;
  font-size: 30rpx;
}

.item{
  width: 100%;
  text-align: center;
}

.buttom_linear{
  color: #41BA53;
  display: inline-block;
  line-height: 50rpx;
  height: 50rpx;
  border-bottom: 2rpx solid #41BA53;
}

/* 商品列表 */

.productList{
  width: 750rpx;
  display: flex;
  flex-direction: row;
  margin-top: 30rpx;
}

.productList_item{
  width: 100%;
  height:160rpx;
  padding: 35rpx 30rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  border-bottom: 2rpx solid #E6E6E6;
}

.productList_item_img{
  margin-right: 30rpx;
}

.productList_item_img image{
  width: 160rpx;
  height: 160rpx;
}

.product_info{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding-top: 10rpx;
}

.product_info_tit{
  /* width: 426rpx; */
  width: 100%;
  text-align: left;
  display: block;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.product_info_price{
  /* margin-top: 80rpx; */
  margin-right: 20rpx;
  width: 490rpx;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.product_info_price_item{
  width: 100%;
  font-size: 28rpx;
  color: #E05E2C;
}

.product_info_price image{
  width: 44rpx;
  height: 44rpx;
}

.buttons{
  width: 100%;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.buttons view{
  width: 160rpx;
  height: 50rpx;
  color: white;
  background-color: #41BA53;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* 现金券兑换 */

.cash{
  width: 690rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  margin: 0 auto;
  padding-bottom: 30rpx;
}

.cash_item{
  width: 330rpx;
  height: 314rpx;
  box-sizing: border-box;
  padding: 30rpx;
  margin: 30rpx 0 0 0;
  background-color: white;
  position: relative;
}

.cash_item_top{
  width: 276rpx;
  height: 134rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #41BA53;
}

.cash_item_top_NOSILL_FULL_REDUCE{
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.cash_item_top_DISCOUNT{
  background-image: url(data:image/png;base64,);
  background-size: 100%;
}

.cash_item_content{
  width: 100%;
  height: 60rpx;
  color: #333333;
  font-size: 24rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.cash_item_contents{
  width: 100%;
  height: 60rpx;
  color: #333333;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cash_item_contents_left{
  width: 145rpx;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.cash_item_contents_right{
  width: 100rpx;
  height: 40rpx;
  color: white;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #41BA53;
  border-radius: 25rpx;
}

.cash_item_buttom{
  width: 100%;
  height: 60rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cash_item_buttom_left{
  width: 100%;
  height: 60rpx;
  color: #41BA53;
  display: flex;
  align-items: center;
}

.cash_item_buttom_right{
  width: 100%;
  height: 40rpx;
  font-size: 24rpx;
  background-color: #41BA53;
  color: white;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.limit{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 83rpx;
  height: 83rpx;
}

/* 兑换记录 */

.record{
  width: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  background-color: white;
}

.record_item{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx;
  border-bottom: 2rpx solid #E6E6E6;
  font-size: 30rpx;
  position: relative;
}

.record_left{
  padding-left: 10rpx;
  box-sizing: border-box;
}

.record_left_buttom{
  color: #999999;
  margin-top: 20rpx;
}

/* 商品详情弹框 */

.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  bottom: 35%;
  width: 650rpx;
  height: 750rpx;
  margin-left: 50rpx;
  background: #f8f8f8;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
  /* box-shadow: 0px 3px 3px 2px gainsboro; */
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}

.swiper_item {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.product_info {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  background-color: white;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
}

.product_tit {
  width: 100%;
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 5rpx;
}

.product_price {
  padding: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  margin-top: 20rpx;
}

.left {
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.content{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.right {
  width: 20%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.left image{
  width: 44rpx;
  height: 30rpx;
}

.right image {
  width: 158rpx;
  height: 44rpx;
}

.content image{
  width: 30rpx;
  height: 30rpx;
}

.popup-address{
  position: fixed;
  background: rgba(0, 0, 0, 0.4);
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  color: #333;
}
.popup-address-content{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
  height: 400rpx;

}
.popup-address-top{
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  height: 80rpx;
  border-bottom: 1px solid #ccc;
}
.popup-address-top text{
  padding: 0 20rpx;
}
.popup-address-b{
  width: 100%;
  height: 320rpx;
  overflow: hidden;
}
.address-text{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  text-align: center;
}
.popup-address-b .active{
  color: #41BA53;
}